<h2>Classes</h2>
<br>
<hr />
{{#each classes}}

	<div class="card" style="">
	  <div class="card-body">
	    <!-- Title -->
	    <h4 class="card-title">{{title}}</h4>
	    <!-- Text -->
	    <p class="card-text">{{description}}</p>
	    <p>Instructor: <strong>{{instructor}}</strong></p>
	    <a class="button" href="/classes/{{_id}}/details" class="btn btn-primary">Class Details</a>
	  </div>
	</div>
	<br>

	<hr />
{{/each}}