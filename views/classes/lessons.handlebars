<h2>{{class.title}}'s Classes</h2>
<table cellspacing="0" cellpadding="0">
	<thead>
		<tr>
			<th>Lesson Number</th>
			<th>Lesson Title</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		{{#each class.lessons}}
			<tr>
				<td>{{lesson_number}}</td>
				<td>{{lesson_title}}</td>
				<td><a href="/classes/{{../class._id}}/lessons/{{lesson_number}}">View Lesson</a></td>
			</tr>
		{{/each}}
	</tbody>
</table>
<iframe width="640" height="360" src="{{class.videLink}}" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>